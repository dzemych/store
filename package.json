{
  "name": "magazine",
  "version": "1.0.0",
  "description": "TZ magazine",
  "main": "server",
  "scripts": {
    "db": "mongod --dbpath=C:\\data\\db",
    "start": "cross-env NODE_ENV=production node server.js",
    "server": "cross-env NODE_ENV=production nodemon server.js",
    "server:install": "npm install",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "admin": "npm run start --prefix admin",
    "admin:install": "npm install --prefix admin",
    "admin:build": "npm run build --prefix admin",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dzemych/magazine.git"
  },
  "keywords": [
    "magazine",
    "clothes",
    "dzemych"
  ],
  "author": "Dzemych Ivan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dzemych/magazine/issues"
  },
  "homepage": "https://github.com/dzemych/magazine#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.3.0",
    "font-awesome": "^4.7.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.9",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.3",
    "sass": "^1.51.0",
    "slugify": "^1.6.5",
    "validator": "^13.7.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3"
  }
}
